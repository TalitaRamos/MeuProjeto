<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.meuprojeto">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.cadastro$Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.cadastro" />
        <activity android:name=".Activity.cadastroAluno" />
        <activity android:name=".Activity.cadastroProfessor" />
        <activity android:name=".Activity.cadastroProjeto" />
        <activity
            android:name=".Activity.Project_prof"
            android:label="@string/title_activity_project_prof"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Activity.ProjetoAluno"
            android:label="@string/title_activity_projeto_aluno"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>